# -*- mode: yaml -*-
# vim:ts=2:sw=2:ai:si:syntax=yaml
#
# Example configuration to customise local settings
---

# Do not update apt-cache
# update_apt_cache: false

# Do not install zsh
# install_zsh: false

# Use custom Go workspace directory
# golang_path: "go"

# Default Node.js version to install (NVM aliases supported)
# nvm_default_node_version: "lts/*"

# Initialize shell profile scripts
# nvm_init_shell: false

# Make Python 3.7.5 default global version in pyenv
# pyenv_global: "3.7.5"

# Do not configure pyenv shell when using existing dotfiles
# pyenv_init_shell: false

# Google Cloud SDK version
# gcloud_version: "236.0.0"

# Install Cloud SDK from package manager
# gcloud_install_from_package_manager: true

# Do not set up Cloud SDK shell when using existing dotfiles
# gcloud_setup_shell: false

# Do not configure Linuxbew shell when using existing dotfiles
# linuxbrew_init_shell: false

# Install Linuxbrew
# install_linuxbrew: true

# Fix permissions in user home directory
# user_home_fix_permissions: true

